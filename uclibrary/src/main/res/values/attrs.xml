<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="icon" format="reference" />
    <attr name="label" format="string"/>
    <attr name="color" format="color"/>
    <attr name="icons" format="reference" />
    <attr name="activeColor" format="color"/>
    <attr name="labelColor" format="color" />
    <attr name="layoutId" format="reference" />
    <attr name="actionItemStyle" format="enum">
        <enum name="iconOnly" value="0"/>
        <enum name="labelOnly" value="1"/>
        <enum name="both" value="2"/>
    </attr>
    <attr name="defaultItem" format="integer" />
    <!-- progress wheel widget options -->
    <declare-styleable name="ProgressWheelView">
        <attr name="tickLineWidth" format="dimension" />
        <attr name="tickLineLength" format="dimension" />
        <attr name="tickLineSpace" format="dimension" />
        <attr name="tickLineColor" format="color" />
        <attr name="middleTickLineColor" format="color" />
        <attr name="totalValue" format="float" />
        <attr name="direction">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="WrapperProgressWheel">
        <attr name="wrapperLayoutId" format="reference"/>
        <attr name="readOnly" format="boolean"/>
        <attr name="withLabel" format="boolean" />
        <attr name="labelId" format="reference" />
        <attr name="progressWheelId" format="reference" />
    </declare-styleable>
    
    <declare-styleable name="OptionBarLayout">
        <attr name="layoutId" />
        <attr name="buttonCancelId" format="reference"/>
        <attr name="buttonApplyId" format="reference"/>
        <attr name="controlContainer" format="reference" />
        <attr name="controlLayoutId" format="reference" />
        <attr name="showButtons" format="boolean" />
    </declare-styleable>

    <declare-styleable name="AspectRatioLayout">
        <attr name="layoutIds" format="reference"/>
        <attr name="ratioBase" format="integer"/>
        <attr name="labels" format="reference"/>
        <attr name="ratios" format="reference"/>
        <attr name="icons" />
        <attr name="activeColor" />
        <attr name="color" />
        <attr name="actionItemStyle" />
        <attr name="defaultItem" />
    </declare-styleable>

    <declare-styleable name="ImageEditorLayout">
        <attr name="useMark" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ActionItemView">
        <attr name="layoutId"/>
        <attr name="icon" />
        <attr name="label" />
        <attr name="activeColor" />
        <attr name="color" />
        <attr name="actionItemStyle" />
    </declare-styleable>
    <declare-styleable name="AspectRatioView">
        <attr name="layoutId"/>
        <attr name="icon" />
        <attr name="label" />
        <attr name="activeColor" />
        <attr name="color" />
        <attr name="actionItemStyle" />
        <attr name="ratioX" format="float"/>
        <attr name="ratioY" format="float"/>
    </declare-styleable>

    <!-- Camera resources below -->


    <declare-styleable name="ListPreference">
        <attr name="key" format="string" />
        <attr name="defaultValue" format="string|reference" />
        <attr name="entryValues" format="reference" />
        <attr name="entries" format="reference" />
        <attr name="labelList" format="reference" />
    </declare-styleable>
    <declare-styleable name="IconIndicator">
        <attr name="icons" />
        <attr name="modes" format="reference" />
    </declare-styleable>
    <declare-styleable name="IconListPreference">
        <!-- If a preference does not have individual icons for each entry, it can has a single icon to represent it. -->
        <attr name="singleIcon" format="reference" />
        <attr name="icons" />
        <attr name="largeIcons" format="reference" />
        <attr name="images" format="reference" />
    </declare-styleable>
    <declare-styleable name="MultiToggleImageButton">
        <attr name="imageIds" format="reference" />
        <attr name="contentDescriptionIds" format="reference" />
    </declare-styleable>
    <declare-styleable name="RadioOptions">
        <attr name="selected_drawable" format="reference" />
    </declare-styleable>

</resources>